@article{manenti_surface_2015,
	Abstract = {We present systematic measurements of the quality factors of surface acoustic wave (SAW) resonators on ST-X quartz in the gigahertz range at a temperature of \$10 {\textbackslash}backslash, {\textbackslash}backslashtextrm\{mK\}\$. We demonstrate a internal quality factor \$Q\_{\textbackslash}backslashmathrm\{i\}\$ approaching \$0.5\$ million at \$0.5 {\textbackslash}backslash, {\textbackslash}backslashtextrm\{GHz\}\$ and show that \$Q\_{\textbackslash}backslashmathrm\{i\}{\textbackslash}backslashgeq4.0{\textbackslash}backslashtimes10{\textasciicircum}4\$ is achievable up to \$4.4 {\textbackslash}backslash, {\textbackslash}backslashtextrm\{GHz\}\$. We show evidence for a polynomial dependence of propagation loss on frequency, as well as a weak drive power dependence of \$Q\_{\textbackslash}backslashmathrm\{i\}\$ that saturates at low power, the latter being consistent with coupling to a bath of two-level systems. Our results indicate that SAW resonators are promising devices for integration with superconducting quantum circuits.},
